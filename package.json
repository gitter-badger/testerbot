{
  "name": "testerbot",
  "version": "0.0.1",
  "description": "Automatic Front-End Testing",
  "main": "./dist/app.js",
  "scripts": {
    "build": "babel ./src --out-dir ./dist --source-maps inline --ignore spec.js,test.js",
    "build-watch": "babel ./src --watch --out-dir ./dist --source-maps inline --ignore spec.js,test.js",
    "start": "node ./dist/index.js",
    "start-debug": "node --inspect-brk ./dist/index.js",
    "test": "jest -i testerbot/tests --config jest.config.js",
    "test-debug": "node --inspect-brk ./node_modules/.bin/jest -i testerbot/tests --config jest.config.js",
    "test-watch": "NODE_ENV=test jest -i testerbot/tests --config jest.config.js --watch --notify",
    "test-dashboard": "NODE_ENV=dashboard npm run test -- --reporters jest-dashboard",
    "test-debug-dashboard": "npm run test-debug -- --reporters jest-dashboard",
    "test-watch-dashboard": "npm run test-watch -- --reporters jest-dashboard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theoutlander/testerbot.git"
  },
  "keywords": [
    "Puppeteer",
    "UI",
    "Test",
    "Selenium",
    "Headless",
    "tests"
  ],
  "author": "Nick Karnik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theoutlander/testerbot/issues"
  },
  "homepage": "https://github.com/theoutlander/testerbot#readme",
  "dependencies": {
    "babel-regenerator-runtime": "^6.5.0",
    "chalk": "^2.3.0",
    "cheerio": "^1.0.0-rc.2",
    "jest": "^22.0.4",
    "jest-dashboard": "^0.2.1",
    "puppeteer": "^0.13.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "prettier-standard": "^8.0.0",
    "standardjs": "^1.0.0-alpha",
    "transform-runtime": "0.0.0"
  }
}
